{
  "manifest_version" : 2,
  "name" : "Our Project",
  "version" : "0.001",
  "background" : {
  "scripts" : 
    ["background.js"]
  },
  "content_scripts" : [
    {
      "matches" : [
      "<all_urls>"
      ],
    "js" : ["scripts/content.js"]
    }],
  "browser_action" : {
      "default_popup" : "popup.html",
      "default_title" : "A popup will come"
  }
}